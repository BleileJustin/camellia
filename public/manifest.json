{
	"manifest_version": 2,
	"name": "__MSG_app_name__",
	"version": "1.0.1",
	"author": "terron",
	"homepage_url": "https://github.com/terron-kun/camellia",
	"description": "__MSG_app_description__",
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
	"short_name": "Camellia",
	"minimum_chrome_version": "51",
	"omnibox": {
		"keyword": "cml"
	},
	"default_locale": "ru",

	"permissions": [
		"bookmarks",
		"storage",
		"notifications",
		"<all_urls>"
	],

	"icons": {
		"16": "/img/logo/16x16-colored.png",
		"32": "/img/logo/32x32-colored.png",
		"64": "/img/logo/64x64-colored.png",
		"128": "/img/logo/128x128-colored.png",
		"256": "/img/logo/256x256-colored.png",
		"512": "/img/logo/512x512-colored.png",
		"1000": "/img/logo/1000x1000-colored.png"
	},

	"chrome_url_overrides" : {
		"newtab": "/newtab.html"
	},

	"options_ui": {
		"page": "/options.html",
		"chrome_style": true
	},

	"background": {
		"scripts": [
			"/js/background.js"
		]
	}
}
