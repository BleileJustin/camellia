{
  "manifest_version": 3,
  "name": "Camellia",
  "description": "Camellia 2.0. Unstable dev build.",
  "short_name": "Camellia",
  "minimum_chrome_version": "80",

  "permissions": ["bookmarks", "storage"],

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none';"
  },

  "icons": {
    "128": "logo.png"
  },

  "chrome_url_overrides": {
    "newtab": "/newtab.html"
  },

  "background": {
    "service_workers": []
  }
}
